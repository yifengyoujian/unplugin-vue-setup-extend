{
  "name": "unplugin-vue-setup-extend",
  "version": "0.3.2",
  "description": "vue setup extend options name",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue",
    "name",
    "extend"
  ],
  "homepage": "https://github.com/yifengyoujian/unplugin-vue-setup-extend",
  "bugs": {
    "url": "https://github.com/yifengyoujian/unplugin-vue-setup-extend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yifengyoujian/unplugin-vue-setup-extend.git"
  },
  "license": "MIT",
  "exports": {
    "./*": "./*",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsup & npm run build:fix",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "example:dev": "npm -C examples/vite run dev",
    "example:build": "npm -C examples/vue-cli run build",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "test:update": "vitest --u"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "magic-string": "^0.26.1",
    "unplugin": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@types/node": "^18.0.0",
    "bumpp": "^8.1.0",
    "eslint": "^8.13.0",
    "esno": "^0.16.3",
    "rollup": "^3.3.0",
    "tsup": "^6.0.1",
    "typescript": "^4.6.3",
    "vitest": "^0.25.2",
    "vite": "^4.0.4"
  }
}
