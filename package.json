{
  "name": "unplugin-vue-setup-extend",
  "version": "0.1.8",
  "description": "vue setup extend options name",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue",
    "name"
  ],
  "homepage": "https://github.com/yifengyoujian/unplugin-vue-setup-extend",
  "bugs": {
    "url": "https://github.com/yifengyoujian/unplugin-vue-setup-extend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yifengyoujian/unplugin-vue-setup-extend.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "example:dev": "npm -C examples/vite run dev",
    "example:build": "npm -C examples/vue-cli run build",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "test:update": "vitest --u"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.33",
    "magic-string": "^0.26.1",
    "unplugin": "^0.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.2",
    "vitest": "^0.9.4",
    "webpack": "^5.72.0"
  }
}
